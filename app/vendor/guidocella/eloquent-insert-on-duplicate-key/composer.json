{
    "name": "guidocella/laravel-insert-on-duplicate-key",
    "type": "library",
    "description": "Macros for INSERT ON DUPLICATE KEY UPDATE and INSERT IGNORE with Eloquent",
    "keywords": ["laravel", "eloquent", "database", "mysql"],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "^5.4||^6||^7"
    },
    "require-dev": {
        "laravel/laravel": "^7",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "InsertOnDuplicateKey\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InsertOnDuplicateKey\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InsertOnDuplicateKey\\InsertOnDuplicateKeyServiceProvider"
            ]
        }
    }
}
